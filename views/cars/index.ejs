<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <side>
        <a href="/">Home</a>
        <a href="/cars/new">Add Car</a>
    </side>

    <h1>Inventory</h1>

    <ul>
        <% cars.forEach( ( car, idx ) => { %>
            <li id='car<%= idx%>'>
                <a href="/cars/<%= idx %>"><%= car.year %> <%= car.make %>-<%= car.model %></a>
            </li>
        <% }) %>
    </ul>
</body>
</html>